[
    {
        "sink": "stdout",
        "level": "DEBUG"
    },
    {
        "sink": "../log/gunicorn.log",
        "rotation": "00:00",
        "retention": "10 days",
        "format": "{time:YYYY-MM-DD HH:mm:ss.SSS} | {level: <8} | {process: <5} | {name}:{function}:{line}{extra[padding]} - {message}",
        "filter": "gunicorn",
        "level": "INFO"
    }
]
